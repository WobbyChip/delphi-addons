function EnableBluetoothConnectable(bool: Boolean): Boolean;
function IsBluetoothEnabled: Boolean;
function IsBluetoothDiscoverable: Boolean;
function IsBluetoothConnectable: Boolean;

implementation

function GetBluetoothStatus: DWORD;
var
  FileName: WideString;
begin
  FileName := GetTempDirectory + 'Bluetooth.exe';
  if Is64Bit then SaveByteArray_const(BluetoothX64, FileName) else SaveByteArray_const(BluetoothX86, FileName);
  Result := ExecuteWait(FileName, 'get', SW_HIDE);
  DeleteFileW(PWideChar(FileName));
end;


function EnableBluetooth(bool: Boolean): DWORD;
var
  FileName: WideString;
begin
  FileName := GetTempDirectory + 'Bluetooth.exe';
  if Is64Bit then SaveByteArray_const(BluetoothX64, FileName) else SaveByteArray_const(BluetoothX86, FileName);
  Result := ExecuteWait(FileName, Q(bool, 'set 1', 'set 0'), SW_HIDE);
  DeleteFileW(PWideChar(FileName));
end;


function EnableBluetoothDiscovery(bool: Boolean): Boolean;
var
  pbtfrp: BLUETOOTH_FIND_RADIO_PARAMS;
  hRadio, hFind: THandle;
begin
  Result := False;
  pbtfrp.dwSize := SizeOf(BLUETOOTH_FIND_RADIO_PARAMS);
  hFind := BluetoothFindFirstRadio(@pbtfrp, hRadio);

  if (hFind <> 0) then begin
    Result := BluetoothEnableDiscovery(hRadio, bool);
    BluetoothIsDiscoverable(hRadio);
    BluetoothFindRadioClose(hFind);
  end;
end;


function EnableBluetoothConnectable(bool: Boolean): Boolean;
var
  pbtfrp: BLUETOOTH_FIND_RADIO_PARAMS;
  hRadio, hFind: THandle;
begin
  Result := False;
  pbtfrp.dwSize := SizeOf(BLUETOOTH_FIND_RADIO_PARAMS);
  hFind := BluetoothFindFirstRadio(@pbtfrp, hRadio);

  if (hFind <> 0) then begin
    Result := BluetoothEnableIncomingConnections(hRadio, bool);
    BluetoothIsConnectable(hRadio);
    BluetoothFindRadioClose(hFind);
  end;
end;


function IsBluetoothEnabled: Boolean;
begin
  Result := (GetBluetoothStatus = 1);
end;


function IsBluetoothDiscoverable: Boolean;
begin
  Result := BluetoothIsDiscoverable(0);
end;


function IsBluetoothConnectable: Boolean;
begin
  Result := BluetoothIsConnectable(0);
end;


end.
